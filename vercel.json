{
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET" }
      ]
    },
    {
      "source": "/embed.html",
      "headers": [
        { "key": "X-Frame-Options", "value": "ALLOW-FROM *" },
        { "key": "Content-Security-Policy", "value": "frame-ancestors *;" }
      ]
    }
  ]
}
